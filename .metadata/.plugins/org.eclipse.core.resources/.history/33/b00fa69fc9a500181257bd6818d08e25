public class Stack {
	private int[] array;
	

	public Stack(int i) {
		
	}

	public void push (int value) {
		
	}
	
	public int pop() {
		return 0; 
	}
	
	public int length() {
		return 0; 
	}
	
	public static void main(String[] args) {
		Stack stack = new Stack(100);
		stack.push(5);
		stack.push(3);
		stack.push(5);
		
		// 테스트를 위한 출력
		System.out.println(stack.pop());
		System.out.println(stack.pop());
		System.out.println(stack.pop());
		
		System.out.println(stack.length());
		
	}// main
}
